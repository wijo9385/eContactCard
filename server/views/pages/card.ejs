                </h1>
                <p class="text-secondary m-0 fw-lighter">
                    <%- title %>
                </p>
                <p class="text-center">
                    <%- notes %>
                </p>
            </div>
            <hr class="mt-2 mb-2">
            <% phones.forEach(phone=> { %>
                <div class="mb-1 d-flex justify-content-between" onclick="window.location = 'tel:<%- phone.phone %>'">
                    <div>
                        <p class="text-secondary mb-2 fw-lighter" style="font-size:0.75rem;">
                            <%- phone.type %>
                        </p>
                        <h3 class="fw-semibold fs-5">
                            <%- phone.phone %>
                        </h3>
                    </div>
                    <div class="d-flex flex-column justify-content-center">
                        <svg class="bi me-2 fs-lighter" width="25" height="25" style="fill:#929292">
                            <use xlink:href="#telephone"></use>
                        </svg>
                    </div>
                </div>
                <hr class="mt-2 mb-2">
                <% }); %>
                    <% emails.forEach(email=> { %>
                        <div class="mb-1 d-flex justify-content-between"
                            onclick="window.location = 'mailto: <%- email.email %>'">
                            <div>
                                <p class="text-secondary m-0 fw-lighter" style="font-size:0.75rem;">
                                    <%- email.type %>
                                </p>
                                <h3 class="fw-semibold fs-5">
                                    <%- email.email %>
                                </h3>
                            </div>
                            <div class="d-flex flex-column justify-content-center">
                                <svg class="bi me-2 fs-lighter" width="25" height="25" style="fill:#929292">
                                    <use xlink:href="#envelope"></use>
                                </svg>
                            </div>
                        </div>
                        <hr class="mt-2 mb-2">
                        <% }); %>
                            <% addresses.forEach(address=> { %>
                                <a style="text-decoration:none; color:inherit;"
                                    href="https://maps.apple.com/?daddr=<%- address.address.address_line1 %>+<%- address.address.address_line2 %>+<%- address.address.city %>+<%- address.address.state %>+<%- address.address.postal %>"
                                    target="blank">
                                    <div class="mb-1 d-flex justify-content-between">
                                        <div>
                                            <p class="text-secondary m-0 fw-lighter" style="font-size:0.75rem;">
                                                <%- address.type %>
                                            </p>
                                            <h3 class="fw-semibold fs-5">
                                                <%- address.address.address_line1 %> <br>
                                                    <%- address.address.city %>, <%- address.address.state %>
                                                            <%- address.address.postal %>
                                            </h3>
                                        </div>
                                        <div class="d-flex flex-column justify-content-center">
                                            <svg class="bi me-2 fs-lighter" width="25" height="25" style="fill:#929292">
                                                <use xlink:href="#top-right-box"></use>
                                            </svg>
                                        </div>
                                    </div>
                                </a>
                                <hr class="mt-2 mb-2">
                                <% }); %>
                                    <div class="row d-flex justify-content-center mt-3">
                                        <div class="col-10">
                                            <div class="row mb-1">
                                                <% socials.forEach(social=> { %>
                                                    <div class="col-4 d-flex justify-content-center p-3">
                                                        <div class="d-flex flex-column justify-content-center">
                                                            <svg class="bi me-2 fs-lighter" width="32" height="32"
                                                                onclick="window.location = '<%- social.url %>'">
                                                                <use xlink:href="#<%- social.type %>"></use>
                                                            </svg>
                                                        </div>
                                                    </div>
                                                    <% }) %>
                                            </div>
                                        </div>
                                    </div>
        </div>
        <a href="/public/file/<%- profile_id %>.vcf" class="btn btn-primary m-3 pb-3 pt-3 fw-lighter fs-4 fixed-bottom"
            style="background-color: black; border-radius: 5rem; border:none;"> <i class="bi bi-person-add"></i> Save
            Contact</a>
    </div>
</body>

</html>